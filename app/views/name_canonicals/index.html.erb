<h1>Listing name_canonicals</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% for name_canonical in @name_canonicals %>
  <tr>
    <td><%=h name_canonical.name %></td>
    <td><%= link_to 'Show', name_canonical %></td>
    <td><%= link_to 'Edit', edit_name_canonical_path(name_canonical) %></td>
    <td><%= link_to 'Destroy', name_canonical, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New name_canonical', new_name_canonical_path %>
