<?xml version="1.0" encoding="UTF-8"?>
<starArchive xmlns="http://rs.tdwg.org/dwc/terms/xsd/archive/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://rs.tdwg.org/dwc/terms/xsd/archive/ http://darwincore.googlecode.com/svn/trunk/text/tdwg_dwc_text.xsd"
    fileRoot=".">
    
    <core encoding="UTF-8" fieldsTerminatedBy="\t" linesTerminatedBy="\n" rowType="http://rs.tdwg.org/dwc/terms/Taxon">
        <files>
            <location>taxa.txt</location>
        </files>
        <!-- the name identifier used in source, if existing a globally unique one -->
        <id term="http://rs.tdwg.org/dwc/terms/TaxonID" index="0"/>
        <field term="http://purl.org/dc/terms/source" index="1"/>
        <!-- a local identifier (often id from the resource database) -->
        <field term="http://purl.org/dc/terms/identifier" index="2"/>
        <field term="http://rs.tdwg.org/dwc/terms/ScientificName" index="3"/>
        <field term="http://rs.tdwg.org/dwc/terms/TaxonRank" index="4"/>
        <!-- enumeration as defined by dwc -->
        <field term="http://rs.tdwg.org/dwc/terms/NomenclaturalCode" index="5"/>
        <field term="http://rs.tdwg.org/dwc/terms/Kingdom" index="6"/>
        <field term="http://purl.org/dc/terms/modified" index="7"/>
    </core>
    
    <extension>
        <files>
            <location>links.txt</location>
        </files>
        <!-- foreign key pointing to the id of the corresponding core record -->
        <coreid term="http://rs.tdwg.org/dwc/terms/TaxonID" index="0"/>
        <!-- the link as a URL -->
        <field term="http://purl.org/dc/terms/source" index="1"/>
        <!-- the MIME type or for xml the full namespace of the most important namespace, e.g. http://rs.tdwg.org/dwc/terms -->
        <field term="http://purl.org/dc/terms/format" index="2"/>
    </extension>
    
    <extension>
        <files>
            <location>lexical_groups.txt</location>
        </files>
        <!-- foreign key pointing to the id of the corresponding core record -->
        <coreid term="http://rs.tdwg.org/dwc/terms/TaxonID" index="0"/>
        <!-- the lexical group ID -->
        <field term="http://purl.org/dc/terms/identifier" index="1"/>
        <!-- qualifier why this name belongs to this lexical group. e.g. orthographical variant -->
        <field term="http://globalnames.org/gni/qualifier" index="2"/>
    </extension>
    
</starArchive>
